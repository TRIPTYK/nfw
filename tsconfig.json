{
    "compilerOptions": {
      "lib": ["ES2020"],
      "target": "ES2020",
      "moduleResolution": "node",
      "module": "CommonJS",
      "allowSyntheticDefaultImports": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "declaration": true,
      "outDir": "./dist",
      "forceConsistentCasingInFileNames": true
    },
    "exclude": [ "dist/**/*"]
  }
  