{
  "compilerOptions": {
    "lib": ["ES2020"],
    "strict": true,
    "target": "ES2020",
    "moduleResolution": "node",
    "module": "ES2020",
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "declaration": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [ "dist/**/*",  "node_modules/**/*"]
}
  