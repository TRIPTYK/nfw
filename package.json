{
  "name": "nfw",
  "version": "0.0.2",
  "description": "This repository contains 3rd party REST API boilerplate Express.js, Typescript, TypeORM based.",
  "scripts": {
    "todo": "leasot -x 'src/**/*.ts'",
    "setup": "npm i && node ./init_scripts/setup",
    "deploy": "npm i && node ./init_scripts/deploy",
    "typedoc": "typedoc --out ./docs --ignoreCompilerErrors",
    "start": "node ./dist/src/app.bootstrap",
    "start-dev": "./node_modules/.bin/ts-node-dev --respawn --transpileOnly ./src/app.bootstrap.ts --env developement",
    "todo-ci": "leasot -x --reporter markdown 'src/**/*.ts' > TODO.md"
  },
  "engines": {
    "node": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TRIPTYK/nfw.git"
