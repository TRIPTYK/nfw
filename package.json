{
  "name": "nfw",
  "version": "0.1.1",
  "description": "This repository contains 3rd party REST API boilerplate Express.js, Typescript, TypeORM based.",
  "scripts": {
    "setup": "npm i && node ./init_scripts/setup",
    "deploy": "npm i && node ./init_scripts/deploy",
    "lint": "./node_modules/.bin/eslint src/**/**/*.ts",
    "migration-generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n ",
    "migration-run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
    "typedoc": "typedoc --out ./docs --ignoreCompilerErrors",
    "start": "./node_modules/.bin/tsc && node ./dist/src/app.bootstrap --env production",
    "test": "./node_modules/.bin/tsc && ./node_modules/.bin/mocha ./dist/test/*.js --reporter spec --timeout 10000 --colors --exit",
    "start-dev": "./node_modules/.bin/ts-node-dev --respawn --transpileOnly ./src/app.bootstrap.ts --env development"
  },
  "engines": {
    "node": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TRIPTYK/nfw.git"
  },
  "keywords": [
    "node",
    "express.js",
    "boilerplate",
    "typescript",
    "json-api",
    "typeorm"
  ],
  "author": "TRIPTYK SPRL",
  "contributors": [
    "Verliefden Romain",
    "Deflorenne Amaury",
    "Antoine Sam",
    "Cardon Sebastien",
    "Bertrand Gilles"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TRIPTYK/nfw/issues"
  },
  "homepage": "https://github.com/TRIPTYK/nfw#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@hapi/boom": "^8.0.0",
    "@hapi/joi": "^17.0.2",
    "axios": "^0.19.1",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "chai-http": "^4.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-spawn": "latest",
    "dashify": "^2.0.0",
