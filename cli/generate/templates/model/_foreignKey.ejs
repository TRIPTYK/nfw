@<%- foreignKey.type %>(type => <%- entityCapitalize %>,<%- entityLowercase %> => <%- entityLowercase %>.<%- foreignKey.REFERENCED_COLUMN_NAME %>)
@JoinColumn({ name: '<%- foreignKey.COLUMN_NAME %>' , referencedColumnName: '<%- foreignKey.type %>' })
<%- foreignKey.COLUMN_NAME %> : <%- foreignKey.type == 'OneToMany' || foreignKey.type == 'ManyToMany' ? `${capitalizeEntity(foreignKey.REFERENCED_COLUMN_NAME)}[]` : capitalizeEntity(foreignKey.REFERENCED_COLUMN_NAME) %>
